<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:TimeCalculator.Styles">
  <!--#region Colors-->
  <SolidColorBrush x:Key="Brushes.MainWindow.Background.Default" Color="{StaticResource Color.MainWindow.Background}"/>
  <!--#endregion-->

  <Style x:Key="Styles.MainWindow" TargetType="DockPanel">
    <Setter Property="Background" Value="{StaticResource Brushes.MainWindow.Background.Default}"/>
  </Style>
</ResourceDictionary>